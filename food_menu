#include<stdio.h>
main()
{
	printf("Pick An Item To Eat: \n1. Pizza,Rs239 \n2. Pasta,Rs179 \n3. Sandwich,Rs149 \n4. Burger,Rs129 \n5. French Fries,Rs99\n");
	int food=0;
	scanf("%d",&food);
	switch(food)
	{
		case 1:
			printf("You choose to eat Pizza And Its Price Is Rs 239.");
			break;
		case 2:
			printf("You choose to eat Pasta And Its Price Is Rs 179.");
			break;
		case 3:
			printf("You choose to eat Sandwich And Its Price Is Rs 149.");
			break;
		case 4:
			printf("You choose to eat Burger And Its Price Is Rs 129.");
			break;
		case 5:
			printf("You choose to eat French Fries And Its Price Is Rs 99.");
			break;
		default: printf("Invalid Choice");
	}
	return 0;
}
